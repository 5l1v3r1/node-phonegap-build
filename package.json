{
    "name": "phonegap-build",
    "description": "PhoneGap Build command-line interface and node.js library.",
    "version": "0.4.0",
    "homepage": "http://github.com/mwbrooks/phonegap-build-cli",
    "repository": {
        "type": "git",
        "url": "git://github.com/mwbrooks/phonegap-build-cli.git"
    },
    "keywords": [
        "phonegap",
        "build",
        "phonegap-build",
        "phonegapbuild",
        "cordova"
    ],
    "preferGlobal": "true",
    "main": "./lib/phonegap-build.js",
    "bin": {
        "phonegap-build": "./bin/phonegap-build.js",
        "pgb": "./bin/phonegap-build.js"
    },
    "scripts": {
        "test": "./node_modules/.bin/jasmine-node --color --verbose spec"
    },
    "engines": {
        "node": ">=0.8.0"
    },
    "dependencies": {
        "colors": "0.6.x",
        "prompt": "0.2.x",
        "shelljs": "0.0.x",
        "optimist": "0.3.x",
        "qrcode-terminal": "0.8.x",
        "phonegap-build-api": "0.3.x"
    },
    "devDependencies": {
        "jasmine-node": "1.1.x"
    },
    "optionalDependencies": {
    },
    "contributors": [{
        "name": "Michael Brooks",
        "email": "michael@michaelbrooks.ca",
        "url": "http://michaelbrooks.ca/"
    },
