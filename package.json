        "phonegap-build": "./bin/phonegap-build.js",
        "pgb": "./bin/phonegap-build.js"
    },
    "scripts": {
        "test": "./node_modules/.bin/jasmine-node --color --verbose spec"
    },
    "engines": {
        "node": ">=0.8.0"
    },
    "dependencies": {
        "prompt": "0.2.x",
        "shelljs": "0.0.x",
        "cordova": "0.1.x",
        "optimist": "0.3.x",
        "phonegap-build-rest": ">=0.3.x"
    },
    "devDependencies": {
        "jasmine-node": "1.x.x"
    },
    "optionalDependencies": {
    },
    "contributors": [{
        "name": "Michael Brooks",
        "email": "michael@michaelbrooks.ca",
        "url": "http://michaelbrooks.ca/"
    }]
